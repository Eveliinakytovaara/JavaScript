 // teht. 8

 let av = parseInt(prompt("Anna aloitus vuosiluku: "))
 let lv = parseInt(prompt("Anna lopetus vuosiluku: "))

 let html = '<ul>';
 for (let v = av; v <= lv; v++)
 {
   if ((v % 4 == 0 && v % 100 != 0) || (v % 400 == 0))
   {
    html += '<li>';
    html += v;
    html += '</li>'
   }
 }
 html += '</ul>';

 kohde.innerHTML = html
